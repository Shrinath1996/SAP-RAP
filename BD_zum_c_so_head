projection;
strict ( 2 );
use draft;

define behavior for zum_c_so_head alias Head
{
  use create;
  use update;
  use delete;

  use action Activate;
  use action Discard;
  use action Edit;
  use action Resume;
  use action Prepare;

  use association _Item { create; with draft; }
}

define behavior for zum_c_so_item alias Item
{
  use update;
  use delete;

  use association _Head { with draft; }
}
